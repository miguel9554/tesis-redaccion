@article{protiva2009,
author = {Protiva, Pavel and Mrkvica, Jan and Macháč, Jan},
title = {A compact step recovery diode subnanosecond pulse generator},
journal = {Microwave and Optical Technology Letters},
volume = {52},
number = {2},
pages = {438-440},
keywords = {Ultra-wideband, pulse generator, Gaussian pulse, step recovery diode, pulse-forming network},
doi = {https://doi.org/10.1002/mop.24945},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mop.24945},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/mop.24945},
abstract = {Abstract A compact Ultra-Wideband subnanosecond pulse generator is described in this article. The fundamental part of the generator consists of a step recovery diode (SRD), which works as a waveform edge sharpener. The SRD is included in a compact pulse-forming network, which is used to form a narrow Gaussian pulse. Theproposed circuit solution utilizes the performance of the SRD effectively with minimal requirements regarding the driver stage of the generator. The measurements show the waveform and power spectrum of the Gaussian pulse that is generated. ©2009 Wiley Periodicals, Inc. Microwave Opt Technol Lett 52: 438–440, 2010; Published online in Wiley InterScience (www.interscience. wiley.com). DOI 10.1002/mop.24945},
year = {2010}
}

@article{kamal2014,
author = {Kamal, Athar and Bhattacharya, Amitabha and Tamrakar, Maruti and Roy, Chayan},
title = {Low-ringing and reduced-cost step recovery diode based UWB pulse generators for GPR applications},
journal = {Microwave and Optical Technology Letters},
volume = {56},
number = {10},
pages = {2289-2294},
keywords = {ground penetrating radar, pulse generator, step recovery diode, ultrawideband, ringing},
doi = {https://doi.org/10.1002/mop.28581},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mop.28581},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/mop.28581},
abstract = {ABSTRACT Low-ringing and reduced-cost pulse generators for ultrawideband applications using step recovery diode (SRD) have been reported in this article. The circuits generate Gaussian pulses of 170 ps pulse width and a minimal ringing of −22 dB and monocycles of 401 ps duration and high fractional bandwidth of 162\%. Cost-reduction is mainly attributed to the resistive attenuator and single SRD used. Negligible change in circuits' performance was witnessed for repetition rate from 1 to 100 MHz which makes it apt for low to medium range ground penetrating radar applications. © 2014 Wiley Periodicals, Inc. Microwave Opt Technol Lett 56:2289–2294, 2014},
year = {2014}
}

